{
	"$schema": "https://json.schemastore.org/tsconfig",
	"extends": "../tsconfig.options.json",
	"compilerOptions": {
		"lib": ["DOM", "DOM.Iterable", "ES2022", "WebWorker"],
		"jsx": "preserve",
		"jsxImportSource": "solid-js",
		"allowJs": true,
		"noEmit": true,
		"baseUrl": ".",
		"paths": {
			"~/*": ["./src/*"]
		}
	},
	"include": ["src/**/*"]
}
