{
	"name": "@playwright-opentelemetry/trace-viewer",
	"version": "0.2.0",
	"type": "module",
	"description": "Playwright OpenTelemetry Trace Viewer - A web application for viewing Playwright Opentelemetry test traces",
	"bin": "./bin/serve.js",
	"files": [
		"bin",
		"dist"
	],
	"scripts": {
		"dev": "vite",
		"build": "tsc && vite build",
		"preview": "vite preview",
		"test:e2e": "pnpm --filter ../reporter build && playwright test",
		"deploy": "pnpx wrangler deploy"
	},
	"keywords": [
		"playwright",
		"opentelemetry",
		"trace-viewer",
		"solidjs"
	],
	"author": "Oliver Stenbom <oliver@endform.dev>",
	"license": "Apache-2.0",
	"packageManager": "pnpm@10.10.0",
	"devDependencies": {
		"@ark-ui/solid": "^5.30.0",
		"@leeoniya/ufuzzy": "^1.0.19",
		"@playwright-opentelemetry/trace-api": "workspace:*",
		"@playwright/test": "^1.57.0",
		"@tailwindcss/vite": "^4.1.18",
		"@types/node": "^25.0.3",
		"@zip.js/zip.js": "^2.8.11",
		"h3": "2.0.1-rc.7",
		"lucide-solid": "^0.562.0",
		"solid-js": "^1.9.10",
		"tailwindcss": "^4.1.18",
		"valibot": "^1.2.0",
		"vite": "^7.3.0",
		"vite-plugin-pwa": "^1.2.0",
		"vite-plugin-solid": "^2.11.10",
		"vitest": "^3.2.4"
	}
}