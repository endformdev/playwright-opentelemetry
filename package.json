{
	"name": "playwright-opentelemetry",
	"version": "0.0.6",
	"description": "A reporter for Playwright that can export traces and spans in opentelemetry format.",
	"type": "module",
	"license": "Apache-2.0",
	"homepage": "https://github.com/endformdev/playwright-opentelemetry#readme",
	"bugs": {
		"url": "https://github.com/endformdev/playwright-opentelemetry/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/endformdev/playwright-opentelemetry"
	},
	"author": "Oliver Stenbom <oliver@endform.dev>",
	"keywords": [
		"playwright",
		"opentelemetry",
		"reporting",
		"testing",
		"automation"
	],
	"files": [
		"dist"
	],
	"main": "./dist/index.mjs",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.mts",
	"exports": {
		".": "./dist/index.mjs",
		"./package.json": "./package.json"
	},
	"scripts": {
		"build": "tsdown",
		"dev": "tsdown --watch",
		"test:unit": "vitest",
		"test:e2e": "playwright test",
		"typecheck": "tsc --noEmit",
		"format": "biome format --write"
	},
	"packageManager": "pnpm@10.10.0",
	"devDependencies": {
		"@biomejs/biome": "2.3.10",
		"@playwright/test": "^1.57.0",
		"@types/node": "^25.0.3",
		"tsdown": "^0.18.1",
		"typescript": "^5.9.3",
		"vitest": "^4.0.16"
	}
}
